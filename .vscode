{
    "editor.tabSize": 2,
    "editor.fontSize": 16,
    "editor.dragAndDrop": false,
    "editor.lineHeight": 35,
    "editor.mouseWheelZoom": true,
    "editor.renderIndentGuides": false,
    "editor.smoothScrolling": true,
    "files.autoGuessEncoding": true,
    "files.eol": "\r\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "editor.minimap.enabled": false,
    "diffEditor.renderSideBySide": false,
    "editor.fontLigatures": true,
    "editor.find.seedSearchStringFromSelection": false,
    "editor.cursorBlinking": "phase",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.feedback.visible": false,
    "workbench.tips.enabled": false,
    "workbench.editor.showTabs": false,
    "workbench.settings.useSplitJSON": true,
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "all",
    "explorer.enableDragAndDrop": false,
    "explorer.openEditors.visible": 15,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "sync.autoUpload": true,
    "sync.quietSync": true,
    "window.zoomLevel": 0,
    "editor.fontFamily": "'Source Code Pro', Consolas, 'Courier New', monospace",
    "window.newWindowDimensions": "inherit",
    "workbench.editor.closeEmptyGroups": false,
    "terminal.integrated.shell.linux": "/usr/bin/tmux",
    "workbench.colorTheme": "Material Theme Lighter High Contrast",
    "files.exclude": {
        "**/.git/objects": true,
        "**/.git/subtree-cache": true,
        "**/node_modules": true
    },
    "keyboard.dispatch": "keyCode",
    "workbench.tree.renderIndentGuides": "none",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
    "editor.roundedSelection": false,
    "editor.cursorSmoothCaretAnimation": true,
    "vim.camelCaseMotion.enable": true,
    "vim.useSystemClipboard": true,
    "search.location": "sidebar",
    "diffEditor.ignoreTrimWhitespace": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.activityBar.visible": true,
    "explorer.autoReveal": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.iconTheme": null
  }
